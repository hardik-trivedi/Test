import java.util.Scanner;

public class IfElseIfDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println("Please enter marks ->");
		Scanner scn = new Scanner(System.in);
		// int marks = 55;
		int marks = scn.nextInt();

		/*
		 * if (marks >= 70) { System.out.println("Distinction"); } else { if
		 * (marks >= 60) { System.out.println("First Class"); } else { if (marks
		 * >= 50) { System.out.println("Second"); } else { if (marks >= 40) {
		 * System.out.println("Pass"); } else { System.out.println("Fail"); } }
		 * } }
		 */

		if (marks >= 70) {
			System.out.println("Distinction");
		} else if (marks >= 60) {
			System.out.println("First Class");
		} else if (marks >= 50) {
			System.out.println("Second");
		} else if (marks >= 40) {
			System.out.println("Pass");
		} else {
			System.out.println("Fail");
		}
	}

}
